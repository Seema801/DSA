#include <iostream>
using namespace std;

bool linearSearch(int arr[], int size, int target) {
    for (int i = 0; i < size; i++) {
        if (arr[i] == target)
            return true;
    }
    return false;
}

int main() {
    int arr[] = {1, 3, 5, 7, 9};
    int target = 5;
    
    if (linearSearch(arr, 5, target))
        cout << "Element found";
    else
        cout << "Element not found";
    
    return 0;
}
